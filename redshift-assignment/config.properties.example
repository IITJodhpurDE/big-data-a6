# Configuration file for database connection
# Copy this to config.properties and fill in your values

# Redshift Connection Details
# Format: jdbc:redshift://your-cluster-endpoint:5439/dev
redshift.url=jdbc:redshift://YOUR_CLUSTER_ENDPOINT:5439/dev

# Database Credentials
redshift.username=admin
redshift.password=YourPassword123

# Optional: Connection Pool Settings
redshift.max.connections=10
redshift.timeout=30
